 <div class="userTableWrap">
  <table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>EMAIL</th>
                <th>ROLE</th>
                              
            </tr>
        </thead>

        <tbody>
        <% @all_user.each do |user| %>
            <tr>
                <td><%= user.email%></td>
                <td><%= user.roles.first.name %></td>
              
            </tr>
            <% end %>
            </tbody>
        </table>
</div>